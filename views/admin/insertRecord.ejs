<body class="">
  <div class="wrapper ">
    
    <% include slideBar %>

    <div class="main-panel">
      
      <% include navBar %>

      <div class="content">
        <div class="container-fluid">
          <form action="/insertRecord" method="post">
            <div class="row">
              <div class="col-md-8">
                <div class="card">
                  <%if (conditional == 1) { %>
                  <div class="alert alert-danger" role="alert">
                    Các trường thông tin không được bỏ trống!
                  </div>
                  <% } %>
                  <%if (conditional == 2) { %>
                  <div class="alert alert-danger" role="alert">
                    Số chứng minh thư đã tồn tại!
                  </div>
                  <% } %>
                  <%if (conditional == 3) { %>
                  <div class="alert alert-success" role="alert">
                    Thêm thành công!
                  </div>
                  <% } %>
                  <div class="card-header card-header-primary">
                    <h4 class="card-title">Insert Record</h4>
                    <p class="card-category">Complete Record</p>
                  </div>
                  <div class="card-body">
                    <form>
                      <div class="row">
                        <div class="col-md-7">
                          <div class="form-group">
                            <label class="bmd-label-floating">Họ và tên</label>
                            <input type="text" id="ten" name="ten" class="form-control" required>
                          </div>
                        </div>
                        <div class="col-md-5">
                          <div class="form-group">
                            <label class="bmd-label-floating">Số CMND</label>
                            <input type="number" id="so_cmt" name="so_cmt" class="form-control">
                          </div>
                        </div>
                      </div>
                      <div class="row">
                        <div class="col-md-6">
                          <div>Ngày sinh :</div>
                          <div class="form-group">
                            <input type="date" id="ngay_sinh" name="ngay_sinh" class="form-control" required>
                          </div>
                        </div>
                        <div class="col-md-6">
                          <div>Ngày cấp CMND :</div>
                          <div class="form-group">
                            <input type="date" id="ngay_cap" name="ngay_cap" class="form-control" required>
                          </div>
                        </div>
                      </div>
                      <br>
                      <div class="row">
                        <div class="col-md-4">
                          <div>Dân tộc</div>
                          <select id="dan_toc" name="dan_toc" class="custom-select">
                            <% dan_toc.forEach(function(dtoc) { %>
                            <option value="<%= dtoc.dt %>"><%= dtoc.dt %></option>
                            <% }); %>
                          </select>
                        </div>
                        <div class="col-md-4">
                          <div>Tôn giáo</div>
                          <select id="ton_giao" name="ton_giao" class="custom-select">
                            <% ton_giao.forEach(function(tgiao) { %>
                            <option value="<%= tgiao.tg %>"><%= tgiao.tg %></option>
                            <% }); %>
                          </select>
                        </div>
                        <div class="col-md-4">
                          <div>Nghề nghiệp</div>
                          <select id="nghe_nghiep" name="nghe_nghiep" class="custom-select">
                            <% nghe_nghiep.forEach(function(nnghiep) { %>
                            <option value="<%= nnghiep.nn %>"><%= nnghiep.nn %></option>
                            <% }); %>
                          </select>
                        </div>
                      </div>
                      <br>
                      <div class="row">
                        <div class="col-md-12">
                          <div class="form-group">
                            <label class="bmd-label-floating">Dấu vêt riêng và dị hình</label>
                            <input type="text" id="dau_vet_rieng_va_di_hinh" name="dau_vet_rieng_va_di_hinh"
                              class="form-control">
                          </div>
                        </div>
                      </div>
                      <br>
                      <p>Nguyên quán</p>
                      <div class="row">
                        <div class="col-md-4">
                          <select id="tp_nguyen_quan" name="tp_nguyen_quan" class="custom-select">
                            <option selected disabled>Tỉnh/Thành phố</option>
                            <option value="Hà Nội">Hà Nội</option>
                            <option value="Hồ Chí Minh">Hồ Chí Minh</option>
                          </select>
                        </div>
                        <div class="col-md-4">
                          <select id="huyen_nguyen_quan" name="huyen_nguyen_quan" class="custom-select">
                            <option selected disabled>Quận/Huyện</option>
                          </select>
                        </div>
                        <div class="col-md-4">
                          <select id="xa_nguyen_quan" name="xa_nguyen_quan" class="custom-select">
                            <option selected disabled>Xã/Phường</option>
                          </select>
                        </div>
                      </div>
                      <br>
                      <p>Thường trú</p>
                      <div class="row">
                        <div class="col-md-4">
                          <select id="tp_thuong_tru" name="tp_thuong_tru" class="custom-select">
                            <option selected disabled>Tỉnh/Thành phố</option>
                            <option value="Hà Nội">Hà Nội</option>
                            <option value="Hồ Chí Minh">Hồ Chí Minh</option>
                          </select>
                        </div>
                        <div class="col-md-4">
                          <select id="huyen_thuong_tru" name="huyen_thuong_tru" class="custom-select">
                            <option selected disabled>Quận/Huyện</option>
                          </select>
                        </div>
                        <div class="col-md-4">
                          <select id="xa_thuong_tru" name="xa_thuong_tru" class="custom-select">
                            <option selected disabled>Xã/Phường</option>
                          </select>
                        </div>
                      </div>
                      <br>
                      <p>Tạm trú</p>
                      <div class="row">
                        <div class="col-md-4">
                          <select id="tp_tam_tru" name="tp_tam_tru" class="custom-select">
                            <option selected disabled>Tỉnh/Thành phố</option>
                            <option value="Hà Nội">Hà Nội</option>
                            <option value="Hồ Chí Minh">Hồ Chí Minh</option>
                          </select>
                        </div>
                        <div class="col-md-4">
                          <select id="huyen_tam_tru" name="huyen_tam_tru" class="custom-select">
                            <option selected disabled>Quận/Huyện</option>
                          </select>
                        </div>
                        <div class="col-md-4">
                          <select id="xa_tam_tru" name="xa_tam_tru" class="custom-select">
                            <option selected disabled>Xã/Phường</option>
                          </select>
                        </div>
                      </div>
                      <br>
                      <!-- <p>Ảnh chân dung</p>
                      <div class="row">
                        <div class="col-md-12">
                          <form>
                            <div class="custom-file">
                              <input type="file" class="custom-file-input" id="anh_chan_dung" name="anh_chan_dung"
                                required>
                              <label class="custom-file-label" for="customFile">Chọn ảnh</label>
                            </div>
                          </form>
                        </div>
                      </div> -->
                      <!-- <br>
                      <p>Ảnh CMND mặt trước</p>
                      <div class="row">
                        <div class="col-md-12">
                          <div class="custom-file">
                            <input type="file" class="custom-file-input" id="anh_cmt_truoc" name="anh_cmt_truoc">
                            <label class="custom-file-label" for="customFile">Chọn ảnh</label>
                          </div>
                        </div>
                      </div>
                      <br>
                      <p>Ảnh CMND mặt sau</p>
                      <div class="row">
                        <div class="col-md-12">
                          <div class="custom-file">
                            <input type="file" class="custom-file-input" id="anh_cmt_sau" name="anh_cmt_sau">
                            <label class="custom-file-label" for="customFile">Chọn ảnh</label>
                          </div>
                        </div>
                      </div> -->
                      <br>
                      <button type="submit" class="btn btn-primary pull-right">Submit</button>
                    </form>
                  </div>
                </div>
              </div>
              <div class="col-md-4">
                <div class="card-body">
                  <h4 class="card-title">Ảnh chân dung</h4>
                  <input type='file' id="anh_chan_dung" name="anh_chan_dung" required />
                  <img id="anh1" src="./images/person.jpg" alt="your image" width="200px" height="200px" />
                </div>
                <div class="card-body">
                  <h4 class="card-title">Ảnh CMND mặt trước</h4>
                  <input type='file' id="anh_cmt_truoc" name="anh_cmt_truoc" required />
                  <img id="anh2" src="./images/card.png" alt="your image" width="200px" height="200px" />
                </div>
                <div class="card-body">
                  <h4 class="card-title">Ảnh CMND mặt sau</h4>
                  <input type='file' id="anh_cmt_sau" name="anh_cmt_sau" required />
                  <img id="anh3" src="./images/card.png" alt="your image" width="200px" height="200px" />
                </div>
              </div>
            </div>
          </form>
        </div>
      </div>
      <footer class="footer">
        <div class="container-fluid">
          <nav class="float-left">
            <ul>
              <li>
                <a href="https://github.com/TorRient/electronic-id-card">
